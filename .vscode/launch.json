{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Electron: npm start",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["start"],
      "windows": {
        "runtimeExecutable": "npm.cmd"
      },
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "ELECTRON_DISABLE_SECURITY_WARNINGS": "true"
      }
    }
  ]
}
